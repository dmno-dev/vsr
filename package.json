{
  "name": "vsr",
  "version": "0.1.1",
  "license": "FSL-1.1-MIT",
  "author": "vlt technology inc. <support@vlt.sh> (http://vlt.sh)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vltpkg/vsr.git"
  },
  "bin": {
    "vsr": "vsr"
  },
  "scripts": {
    "setup": "wrangler d1 execute vsr_local_database --file ./src/db/setup.sql --local --persist-to local-store --experimental-json-config",
    "drop": "wrangler d1 execute vsr_local_database --file ./src/db/drop.sql --local --persist-to local-store --experimental-json-config",
    "migrate": "wrangler d1 migrations apply vsr_local_database  --local --persist-to local-store --experimental-json-config",
    "dev": "wrangler dev --local --persist-to local-store --experimental-json-config",
    "deploy": "wrangler deploy",
    "build": "NODE_NO_WARNINGS=1 node ./scripts/build.mjs",
    "lint": "prettier .",
    "postinstall": "npm run setup"
  },
  "devDependencies": {
    "libnpmpack": "^7.0.4",
    "npm-registry-fetch": "^17.1.0",
    "prettier": "^3.3.3",
    "ssri": "^10.0.6"
  },
  "dependencies": {
    "@hono-rate-limiter/cloudflare": "^0.2.1",
    "@scalar/hono-api-reference": "^0.5.158",
    "hono": "^4.5.5",
    "hono-rate-limiter": "^0.4.0",
    "js-yaml": "^4.1.0",
    "semver": "^7.6.3",
    "streaming-tarball": "^1.0.3",
    "uuid": "^10.0.0",
    "validate-npm-package-name": "5.0.0",
    "wrangler": "^3.84.1"
  },
  "devEngines": {
    "runtime": {
      "name": "node",
      "onFail": "warn"
    },
    "packageManager": {
      "name": "npm",
      "onFail": "warn"
    }
  },
  "engines": {
    "node": ">=22.11.0",
    "npm": ">=10.9.0"
  }
}
