{
  "name": "vsr",
  "version": "0.1.1",
  "private": true,
  "bin": {
    "vsr": "vsr"
  },
  "scripts": {
    "setup": "wrangler d1 execute vsr --local --file ./src/db/setup.sql",
    "drop": "wrangler d1 execute vsr --local --file ./src/db/drop.sql",
    "migrate": "wrangler d1 migrations apply vsr --local",
    "dev": "wrangler dev --persist-to local-store",
    "deploy": "wrangler deploy",
    "build": "NODE_NO_WARNINGS=1 node ./scripts/build.mjs",
    "lint": "prettier ."
  },
  "devDependencies": {
    "libnpmpack": "^7.0.4",
    "npm-registry-fetch": "^17.1.0",
    "prettier": "^3.3.3",
    "ssri": "^10.0.6"
  },
  "dependencies": {
    "@scalar/hono-api-reference": "^0.5.158",
    "hono": "^4.5.5",
    "hono-rate-limiter": "^0.4.0",
    "js-yaml": "^4.1.0",
    "semver": "^7.6.3",
    "streaming-tarball": "^1.0.3",
    "uuid": "^10.0.0",
    "validate-npm-package-name": "5.0.0",
    "wrangler": "^3.83.0"
  },
  "devEngines": {
    "runtime": {
      "name": "node",
      "onFail": "warn"
    },
    "packageManager": {
      "name": "npm",
      "onFail": "warn"
    }
  },
  "engines": {
    "node": ">=22.11.0",
    "npm": ">=10.9.0"
  }
}
